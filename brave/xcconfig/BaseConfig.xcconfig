#include "./local-def.xcconfig"
#include "./build-id.xcconfig"
HEADER_SEARCH_PATHS = ./brave/ThirdParty/leveldb/include ./brave/ThirdParty/leveldb ./brave/ThirdParty/re2

MOZ_BUNDLE_DISPLAY_NAME = Brave$(BETA)
MOZ_PRODUCT_NAME = Brave$(BETA)
MOZ_PRODUCT_ICON = AppIcon$(BETA)

// App Version
MOZ_VERSION = 1.6.6
MOZ_BUILD_ID = $(GENERATED_BUILD_ID)
ProvisioningStyle = Automatic

FRAMEWORK_SEARCH_PATHS = $(PROJECT_DIR) "$(PROJECT_DIR)/Carthage/Build/iOS"

CODE_SIGN_IDENTITY= iPhone Developer

IPHONEOS_DEPLOYMENT_TARGET = 9.0

// The pre-built ssl lib in FXA requires this off
ENABLE_BITCODE = NO

OTHER_SWIFT_FLAGS_Debug = -DDEBUG
OTHER_SWIFT_FLAGS_Release =
OTHER_SWIFT_FLAGS=$(OTHER_SWIFT_FLAGS_$(CONFIGURATION)) -DDISABLE_INTRO_SCREEN -DBRAVE $(LOCAL_DEF_OTHER_SWIFT_FLAGS)

VALIDATE_PRODUCT_Release = YES
VALIDATE_PRODUCT_Debug = NO
VALIDATE_PRODUCT = $(VALIDATE_PRODUCT_$(CONFIGURATION))

ENABLE_TESTABILITY_Release = NO
ENABLE_TESTABILITY_Debug = YES
ENABLE_TESTABILITY = $(ENABLE_TESTABILITY_$(CONFIGURATION))

GCC_PREPROCESSOR_DEFINITIONS_Debug = DEBUG=1
GCC_PREPROCESSOR_DEFINITIONS_Release = NDEBUG=1
GCC_PREPROCESSOR_DEFINITIONS = BRAVE=1 $(GCC_PREPROCESSOR_DEFINITIONS_$(CONFIGURATION)) $(LOCAL_DEF_GCC_PREPROCESSOR_DEFINITIONS)

SWIFT_OPTIMIZATION_LEVEL_Debug = -Onone
SWIFT_OPTIMIZATION_LEVEL_Release = -Owholemodule
SWIFT_OPTIMIZATION_LEVEL = $(SWIFT_OPTIMIZATION_LEVEL_$(CONFIGURATION))


